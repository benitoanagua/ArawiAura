{
  "name": "arawi-aura",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "db:clean": "tsx scripts/clean-db.ts",
    "db:init": "tsx scripts/init-db.ts",
    "db:seed": "tsx -e \"import { seedDevelopmentData } from './src/lib/db/seed.js'; await seedDevelopmentData();\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:theme": "tsx scripts/material-theme/cli.ts",
    "prestart": "npm run generate:theme",
    "prebuild": "npm run generate:theme"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "dotenv": "^16.4.7",
    "surrealdb": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@iconify/svelte": "^5.2.1",
    "@ngneat/falso": "^8.0.2",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/sveltekit": "^10.1.11",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.3",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "^10.1.11",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "storybook": "^10.1.11",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.0"
  }
}